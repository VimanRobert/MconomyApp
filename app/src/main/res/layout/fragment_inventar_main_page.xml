<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".microdir.InventarMainPageFragment">

    <Button
        android:id="@+id/inventar_nou"
        android:layout_width="218dp"
        android:layout_height="72dp"
        android:textSize="18sp"
        android:text="@string/inventar_nou"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/todatabase"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="152dp"
        android:layout_marginBottom="120sp"/>

    <Button
        android:id="@+id/todatabase"
        android:layout_width="218dp"
        android:layout_height="72dp"
        android:layout_marginBottom="250sp"
        android:text="@string/vezi_listele"
        android:textAllCaps="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>